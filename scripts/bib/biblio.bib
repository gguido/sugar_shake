	keywords = {Apis mellifera, Detoxification, Immunity, Pathogen, Pesticide, Toxicology, Varroa mite},
	pages = {1042--1049},
	file = {Gregorc et al_2012_Gene expression in honey bee (Apis mellifera) larvae exposed to pesticides and.pdf:C:\Users\umberto\AppData\Roaming\Mozilla\Firefox\Profiles\k6ov01qp.default\zotero\storage\4ARAEST8\Gregorc et al_2012_Gene expression in honey bee (Apis mellifera) larvae exposed to pesticides and.pdf:application/pdf;science.pdf:C:\Users\umberto\AppData\Roaming\Mozilla\Firefox\Profiles\k6ov01qp.default\zotero\storage\8SNJQP2G\science.pdf:application/pdf;ScienceDirect Snapshot:C:\Users\umberto\AppData\Roaming\Mozilla\Firefox\Profiles\k6ov01qp.default\zotero\storage\UTXKVINS\S0022191012000790.html:text/html;ScienceDirect Snapshot:C:\Users\umberto\AppData\Roaming\Mozilla\Firefox\Profiles\k6ov01qp.default\zotero\storage\6TGR8TCN\S0022191012000790.html:text/html}
