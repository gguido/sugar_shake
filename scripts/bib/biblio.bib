@article{anderson_varroa_2000-1,
	title = {Varroa jacobsoni {(Acari:} Varroidae) is more than one species},
	volume = {24},
	issn = {0168-8162},
	shorttitle = {Varroa jacobsoni {(Acari}},
	url = {http://www.springerlink.com/content/g08668v7h3747153/abstract/},
	doi = {10.1023/A:1006456720416},
	abstract = {Varroa jacobsoni was first described as a natural ectoparasitic mite of the Eastern honeybee ( Apis cerana ) throughout Asia. It later switched host to the Western honeybee ( A. mellifera ) and has now become a serious pest of that bee worldwide. The studies reported here on genotypic, phenotypic and reproductive variation among V. jacobsoni infesting A. cerana throughout Asia demonstrate that V. jacobsoni is a complex of at least two different species. In a new classification V. jacobsoni is here redefined as encompassing nine haplotypes (mites with distinct {mtDNA} {CO-I} gene sequences) that infest A. cerana in the {Malaysia–Indonesia} region. Included is a Java haplotype, specimens of which were used to first describe V. jacobsoni at the beginning of this century. A new name, V. destructor n. sp., is given to six haplotypes that infest A. cerana on mainland Asia. Adult females of V. destructor are significantly larger and less spherical in shape than females of V. jacobsoni and they are also reproductively isolated from females of V. jacobsoni . The taxonomic positions of a further three unique haplotypes that infest A. cerana in the Philippines is uncertain and requires further study.},
	number = {3},
	urldate = {2012-05-08},
	journal = {Experimental and Applied Acarology},
	author = {Anderson, {D.L.} and Trueman, {J.W.H.}},
	year = {2000},
	keywords = {taxonomy, Varroa mite},
	pages = {165--189},
