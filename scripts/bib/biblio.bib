@article{yoo_rapid_2012,
	title = {Rapid detection of sacbrood virus in honeybee using ultra-rapid real-time polymerase chain reaction},
	volume = {179},
	issn = {0166-0934},
	url = {http://www.sciencedirect.com/science/article/pii/S0166093411004307},
	doi = {10.1016/j.jviromet.2011.10.014},
	abstract = {A real-time reverse transcription-polymerase chain reaction {(qRT-PCR)} assay was developed for the fast and highly sensitive detection of the sacbrood virus {(SBV)} genome and applied to honeybee samples. Using plasmid {DNA} containing a partial {SBV} genome and diluted serially, as few as 1\&\#xa0;×\&\#xa0;102\&\#xa0;copies/μl (correlation co-efficiency \&gt;0.99) were detected by the {qRT-PCR} assay, whereas 1\&\#xa0;×\&\#xa0;103\&\#xa0;copies/μl were detected by the conventional {RT-PCR} assay. As a rapid detection method, ultra-rapid real-time {PCR} {(URRT-PCR)} was carried out with a {GenSpector} {TMC-1000} silicon-glass chip-based thermal cycler, which has a 6\&\#xa0;μl micro-chamber volume and a fast outstandingly heating/cooling rate. Using this method, 103\&\#xa0;copies of {pBX-SBV3.8} clone were detected within 17\&\#xa0;min after 40 {PCR} cycles, including melting point analysis. To reduce the detection time for {SBV}, synthesis of the {cDNA} of the {SBV} genome from a honeybee sample was attempted for different reaction times and the {cDNA} was used as the template for {URRT-PCR} assays. The results indicated that a 5\&\#xa0;min reaction time was sufficient to synthesize {cDNA} as the template for the {SBV} {URRT-PCR} assay. This study described a novel {PCR-based} method that is able to detect an {RNA} virus in environmental samples within 22\&\#xa0;min, including reverse transcription, {PCR} detection and melting point analysis in real-time.},
	number = {1},
	urldate = {2012-04-15},
	journal = {Journal of Virological Methods},
	author = {Yoo, Mi-Sun and Thi, Kim Cuc Nguyen and Van Nguyen, Phu and Han, Sang-Hoon and Kwon, Soon-Hwan and Yoon, Byoung-Su},
	month = jan,
	year = {2012},
	keywords = {Apis mellifera, Diagnosis, Reverse transcription-polymerase chain reaction {(RT-PCR)}, Sacbrood virus, Sacbrood virus {(SBV)}, Ultra-rapid real-time {PCR} {(URRT-PCR)}},
	pages = {195--200},
	file = {ScienceDirect Snapshot:C:\Users\umberto\AppData\Roaming\Mozilla\Firefox\Profiles\k6ov01qp.default\zotero\storage\CUVGP3AR\Yoo et al. - 2012 - Rapid detection of sacbrood virus in honeybee usin.html:text/html;Yoo et al_2012_Rapid detection of sacbrood virus in honeybee using ultra-rapid real-time.pdf:C:\Users\umberto\AppData\Roaming\Mozilla\Firefox\Profiles\k6ov01qp.default\zotero\storage\JPQZD4TT\Yoo et al_2012_Rapid detection of sacbrood virus in honeybee using ultra-rapid real-time.pdf:application/pdf}
