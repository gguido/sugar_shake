	journal = {Veterinary Parasitology},
	author = {von Samson-Himmelstjerna, G. and Fritzen, B. and Demeler, J. and Schürmann, S. and Rohn, K. and Schnieder, T. and Epe, C.},
	month = mar,
	year = {2007},
	keywords = {Anthelmintic resistance, count, Cyathostomin, Egg-reappearance period, Faecal egg count reduction test, Ivermectin, Parascaris equorum, Pyrantel, Statistics},
