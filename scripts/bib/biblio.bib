	author = {Krupke, Christian H. and Hunt, Greg J. and Eitzer, Brian D. and Andino, Gladys and Given, Krispn},
	month = jan,
	year = {2012},
	keywords = {Apis mellifera, exposure},
	pages = {e29268},
