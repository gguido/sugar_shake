	number = {21},
	urldate = {2012-09-17},
	journal = {Proceedings of the National Academy of Sciences of the United States of America},
	author = {Yang, Xiaolong and Cox-Foster, Diana L.},
	month = may,
	year = {2005},
	keywords = {2read\_uw, Antimicrobial peptides, Apis mellifera, Immunology, Immunosuppression, Innate immunity, pathology, Varroa mite, Virus},
	pages = {7470--7475},
	file = {Snapshot:C:\Users\umberto\AppData\Roaming\Mozilla\Firefox\Profiles\k6ov01qp.default\zotero\storage\TKRKVS7M\7470.html:text/html;Yang_Cox-Foster_2005_Impact_of_an_ectoparasite_on_the_immunity_and_pathology_of_an_invertebrate.pdf:C:\Users\umberto\AppData\Roaming\Mozilla\Firefox\Profiles\k6ov01qp.default\zotero\storage\9DF62FPR\Yang_Cox-Foster_2005_Impact_of_an_ectoparasite_on_the_immunity_and_pathology_of_an_invertebrate.pdf:application/pdf}
