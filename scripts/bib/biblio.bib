@article{da_cruzlandim_virus_2012,
	title = {Virus present in the reproductive tract of asymptomatic drones of honey bee {(Apis} mellifera l.), and possible infection of queen during mating, Virus present in the reproductive tract of asymptomatic drones of honey bee {(Apis} mellifera l.), and possible infection of queen during mating},
	copyright = {Copyright © 2012 Wiley Periodicals, Inc.},
