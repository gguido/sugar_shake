	pages = {410--419},
	file = {Decourtye_et_al_2004_Effects_of_imidacloprid_and_deltamethrin_on_associative_learning_in_honeybees.pdf:C:\Users\umberto\AppData\Roaming\Mozilla\Firefox\Profiles\k6ov01qp.default\zotero\storage\5BT6MIHX\Decourtye_et_al_2004_Effects_of_imidacloprid_and_deltamethrin_on_associative_learning_in_honeybees.pdf:application/pdf;ScienceDirect Snapshot:C:\Users\umberto\AppData\Roaming\Mozilla\Firefox\Profiles\k6ov01qp.default\zotero\storage\4AEDQT3N\Decourtye et al. - 2004 - Effects of imidacloprid and deltamethrin on associ.html:text/html}
