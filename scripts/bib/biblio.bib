@article{enright_specificity_2004,
	title = {Specificity of Association between {\&lt;i\&gt;Paenibacillus\&lt;/i\&gt;} spp. and the Entomopathogenic Nematodes, {\&lt;i\&gt;Heterorhabditis\&lt;/i\&gt;} spp.},
