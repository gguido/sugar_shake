@article{traniello_development_2002,
	title = {The Development of Immunity in a Social Insect: Evidence for the Group Facilitation of Disease Resistance},
	volume = {99},
	issn = {0027-8424, 1091-6490},
	shorttitle = {The development of immunity in a social insect},
	url = {http://www.pnas.org/content/99/10/6838},
	doi = {10.1073/pnas.102176599},
	abstract = {The extraordinary diversity and ecological success of the social insects has been attributed to their ability to cope with the rich and often infectious microbial community inhabiting their nests and feeding sites. Mechanisms of disease control used by eusocial species include antibiotic glandular secretions, mutual grooming, removal of diseased individuals from the nest, and the innate and adaptive immune responses of colony members. Here we demonstrate that after a challenge exposure to the entomopathogenic fungus Metarhizium anisopliae, dampwood termites Zootermopsis angusticollis have higher survivorship when individuals develop immunity as group members. Furthermore, termites significantly improve their ability to resist infection when they are placed in contact with previously immunized nestmates. This “social transfer” of infection resistance, a previously unrecognized mechanism of disease control in the social insects, could explain how group living may improve the survivorship of colony members despite the increased risks of pathogen transmission that can accompany sociality.},
	language = {en},
	number = {10},
	urldate = {2012-04-30},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Traniello, James F. A and Rosengaus, Rebeca B and Savoie, Keely},
	month = may,
	year = {2002},
	keywords = {Immunology, invertebrate, termites},
	pages = {6838--6842},
	file = {Snapshot:C:\Users\umberto\AppData\Roaming\Mozilla\Firefox\Profiles\k6ov01qp.default\zotero\storage\5TC7V27I\6838.html:text/html;Traniello et al_2002_The Development of Immunity in a Social Insect.pdf:C:\Users\umberto\AppData\Roaming\Mozilla\Firefox\Profiles\k6ov01qp.default\zotero\storage\7VEWQ7G4\Traniello et al_2002_The Development of Immunity in a Social Insect.pdf:application/pdf}
